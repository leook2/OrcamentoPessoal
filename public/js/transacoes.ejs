<%
parseFloatt = function(vetorReceita, vetorDespesa = '') {
    let vetor1 = 0
    let vetor2 = 0

    for(let i=0; i<vetorReceita.length; i++){
        vetor1 =vetor1 + parseFloat(vetorReceita[i])
    }
    for(let i=0; i<vetorDespesa.length; i++){
        vetor2 = vetor2 + parseFloat(vetorDespesa[i])
    }
    const result = vetor1 - vetor2
    var dinheiro = result.toLocaleString('pt-br',{style: 'currency', currency: 'BRL'});
    return dinheiro
}

%>