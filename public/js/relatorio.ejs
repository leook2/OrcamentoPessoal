<%
parseFloatt = function(vetor) {
    let result = 0
    for(let i=0; i<vetor.length; i++){
        result = result + parseFloat(vetor[i])
    }
    var dinheiro = result.toLocaleString('pt-br',{style: 'currency', currency: 'BRL'});
    return dinheiro
}

%>
